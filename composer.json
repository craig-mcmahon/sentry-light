{
    "name": "kodus/sentry",
    "description": "Lightweight alternative to the official Sentry client for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rasmus Schultz",
            "email": "rasc@jfmedier.dk"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0"
